{
  "name": "movies-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --exec babel-node index.js",
    "clean": "rimraf ./build && mkdir build",
    "compile:app": "babel index.js --out-dir build/",
    "compile:movies": "babel ./api/movies --out-dir build/api/movies",
    "compile:users": "babel ./api/users --out-dir build/api/users",
    "compile:genres": "babel ./api/genres --out-dir build/api/genres",
    "compile:tmdb": "babel ./api/tmdb-api.js --out-dir build/api/",
    "compile:middleware": "babel ./middleware --out-dir build/middleware",
    "compile:db": "babel db.js --out-dir build/",
    "compile:data": "babel ./seedData --out-dir build/seedData",
    "compile:authenticate": "babel authenticate.js --out-dir build/",
    "compile:copy": "cp -r ./node_modules/ build/node_modules && cp -r ./public build/public",
    "compile:all": "npm-run-all compile:app compile:genres compile:movies compile:users compile:tmdb compile:middleware compile:db compile:data compile:authenticate compile:copy",
    "build": "npm-run-all clean compile:all",
    "serve": "node ./build/index.js",
    "test": "mocha --timeout 5000"
  },
  "author": "ZhengjieYe <20091571@mail.wit.ie>",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/register": "^7.12.10",
    "babel-eslint": "^10.1.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.0.2",
    "eslint": "^7.14.0",
    "mocha": "^8.1.3",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@optimizely/express": "^0.2.1",
    "bcrypt-nodejs": "0.0.3",
    "chalk": "^4.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "helmet": "^4.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.4",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "swagger-jsdoc": "^6.0.0",
    "swagger-ui-express": "^4.1.6"
  }
}
